{
  "version": "1.0.2",
  "name": "Correction Fluid",
  "manifest_version": 2,
  "description": "Automatically replace the text of a page to what you want.",
  "icons": {
    "16": "assets/img/icon-16.png",
    "48": "assets/img/icon-48.png",
    "128": "assets/img/icon-128.png"
  },
  "browser_action": {
    "default_title": "Add a rule quickly",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "inject.html"
  ],
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": false
  },
  "permissions": [
    "storage",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "key": "",
  "content_security_policy": "default-src 'self'; script-src 'self' https://localhost:3000 'unsafe-eval'; connect-src https://localhost:3000 wss://localhost:3000; style-src * 'unsafe-inline' 'self' blob:; img-src 'self' data:;"
}
